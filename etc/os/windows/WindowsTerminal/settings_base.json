        {
            "command": "newTab",
            "keys": [
                "ctrl+shift+t"
            ]
        },
        {
            "command": "nextTab",
            "keys": [
                "ctrl+tab"
            ]
        },
        {
            "command": "openSettings",
            "keys": [
                "ctrl+,"
            ]
        },
        {
            "command": "prevTab",
